<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_with model: @user, method: :post do |form| %>
  <%= render "basic_inputs", form: form %>
  <p>
  <%= form.label(:password, "Password:") %>
  <%= form.password_field :password %>
  </p>
  <p>
  <%= form.submit %>
  </p>
<% end %>